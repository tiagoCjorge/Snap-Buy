                </div><!-- .End .tab-pane -->
                <div class="tab-pane fade" id="mobile-cats-tab" role="tabpanel" aria-labelledby="mobile-cats-link">
                    <nav class="mobile-cats-nav">
                        <ul class="mobile-cats-menu">
                            <%let teste = 0;%>
                            <%let cat = categoria;%>
                            <%while(teste<cat.length){%>
                            <li><a class="mobile-cats-lead" href="/cat=<%=cat[teste].id%>"><%=cat[teste].nome%></a></li>
                            <%teste+=1}%>
                        </ul><!-- End .mobile-cats-menu -->
                    </nav><!-- End .mobile-cats-nav -->
                </div><!-- .End .tab-pane -->
            </div><!-- End .tab-content -->

        </div><!-- End .mobile-menu-wrapper -->
    </div><!-- End .mobile-menu-container -->

    <div class="modal fade" id="edit-modal" tabindex="-1" role="dialog" aria-hidden="true">
        <div class="modal-dialog modal-dialog-centered" role="document">
            <div class="modal-content">
                <div class="modal-body">
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true"><i class="icon-close"></i></span>
                    </button>

                    <div class="form-box">
                        <div class="form-tab">
                            <ul class="nav nav-pills nav-fill" role="tablist">
                                <li class="nav-item">
                                    <a class="nav-link active" id="edit-tab" data-toggle="tab"  role="tab" aria-controls="signin" aria-selected="true">Finalizar Encomenda</a>
                                </li>
                            </ul>
                            <div class="tab-content" id="tab-content-5">
                                <div class="tab-pane fade show active" id="edit" role="tabpanel" aria-labelledby="edit-tab">
                                    <!--Formulário para finalizar encomenda-->
                                    <form action="/encomenda" method="POST">
                                        <div class="form-group">
                                            <label for="rua"><b>Local de Entrega</b></label>
                                                               <br> <%let rua = dados[0].morada + " - " +  dados[0].codpost+ " , "+  dados[0].cidade+" , "+dados[0].pais%>
                                                                <input type="text" name="rua" id="rua" value="<%=rua%>" hidden>
                                                    <span><%= rua %></span>
                                        </div><!-- End .form-group -->
                                        <div class="form-group">
                                            <label for="pagamento"><b>Metodo de Pagamento</b></label>
                                                                <br>
                                                    <span id="pagamento">Transferencia Bancária </span>
                                        </div><!-- End .form-group -->
                                        <div class="form-group">
                                            <label for="pagamento"><b>Notas da Encomenda</b></label>
                                                                <br>
                                            <textarea name="notas" id="notas" style="resize: none;"> </textarea>
                                        </div><!-- End .form-group -->
                                        <!--input hidden para enviar a informação para o servidor-->
                                        <input type="text" name="encProd" id="encProd" hidden value="<%= JSON.stringify(produtos)%>">
                                      <input type="text" name="encQuant" id="encQuant" hidden value="<%= JSON.stringify(quantidades)%>">
                                      <input type="text" name="estado" id="estado" value="Pendente" hidden>
                                      <input type="text" name="totalEnc" id="totalEnc" value="<%=subtotal + 10%>" hidden >
                                        <div class="form-footer">
                                            <button  type="submit"  class="btn btn-outline-primary-2">
                                                <span>Encomendar</span>
                                                <i class="icon-long-arrow-right"></i>
                                            </button>
                                        </div><!-- End .form-footer -->
                                    </form>
                                </div><!-- .End .tab-pane -->
                            </div><!-- End .tab-content -->
                        </div><!-- End .form-tab -->
                    </div><!-- End .form-box -->
                </div><!-- End .modal-body -->
            </div><!-- End .modal-content -->
        </div><!-- End .modal-dialog -->
    </div><!-- End .modal -->

